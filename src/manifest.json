{
  "manifest_version": 2,
  "name": "Bandwidth Hero++",
  "short_name": "Bandwidth Hero++",
  "description": "Saves data by compressing images on the page.",
  "version": "3.1.1",
  "browser_action": {
    "default_icon": "assets/icon-doge.png",
    "default_title": "Bandwidth Hero++",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.bundle.js"]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
